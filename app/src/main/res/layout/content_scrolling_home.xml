        <TextView
            android:id="@+id/txtlocalFoodTitle"
            style="@style/StandardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/margin_short"
            android:text="غذاهای محلی" />

    </RelativeLayout>
    <!--souvenirHolder-->
    <RelativeLayout
        android:id="@+id/souvenirHomeHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/localFoodHomeHolder"
        android:padding="@dimen/padding_standard"
        android:visibility="visible">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerSouvenir"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtSouvenirTitle" />

        <TextView
            android:id="@+id/txtSouvenirTitle"
            style="@style/StandardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/margin_short"
            android:text="سوغاتی" />
    </RelativeLayout>
    <!--listProvince-->

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerViewProvinceShow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/souvenirHomeHolder"
        android:layout_margin="15dp" />

