<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentEnd="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:descendantFocusability="blocksDescendants">

    <RelativeLayout
        android:id="@+id/SelectHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/selectHoldetTop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/TypeHotelHolder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:translationZ="1dp">

                <ImageView
                    android:id="@+id/imageTypeHotel"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_bed" />

                <TextView
                    android:id="@+id/attractionType"
                    style="@style/StandardTextGrey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageTypeHotel"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:text="اقامت" />
            </RelativeLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/greySemilight" />

            <RelativeLayout
                android:id="@+id/overlapImageHolder2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:translationZ="1dp">

                <ImageView
                    android:id="@+id/imageTypeAttraction"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:src="@mipmap/ic_flight_blue" />

                <TextView
                    android:id="@+id/txtTypeHotel"
                    style="@style/StandardTextGrey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageTypeAttraction"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:text="پرواز" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:id="@id/divider"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/selectHoldetTop"
            android:background="@color/greySemilight" />

        <LinearLayout
            android:id="@+id/hj2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/divider"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/overlapImageHolder1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:translationZ="1dp">

                <ImageView
                    android:id="@+id/imageTypeAttraction2"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_itinerary_blue"
                    android:tint="@color/main_blue" />

                <TextView
                    android:id="@+id/attractionType2"
                    style="@style/StandardTextGrey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageTypeAttraction2"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:text="برنامه سفر" />
            </RelativeLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/greySemilight" />

            <RelativeLayout
                android:id="@+id/TypeAttractionHolder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:translationZ="1dp">

                <ImageView
                    android:id="@+id/imageTypeAttraction1"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_alignEnd="@+id/attractionType1"
                    android:layout_alignParentTop="true"
                    android:padding="6dp"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/ic_attraction_blue" />

                <TextView
                    android:id="@+id/attractionType1"
                    style="@style/StandardTextGrey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageTypeAttraction1"
                    android:layout_centerHorizontal="true"
                    android:padding="5dp"
                    android:text="جاذبه" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:id="@+id/efed"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/hj2"
            android:background="@color/greySemilight" />
    </RelativeLayout>
    <!--viewPagerEventsHolder-->

    <RelativeLayout
        android:id="@+id/viewPagerEventsHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/SelectHolder"
        android:paddingBottom="@dimen/padding_standard"
        android:paddingLeft="@dimen/padding_standard"
        android:paddingRight="@dimen/padding_standard">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_below="@+id/txtEventsTitle"
            android:layout_margin="@dimen/margin_xshort"
            app:cardCornerRadius="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:gravity="center"
                android:layoutDirection="ltr">

                <com.iranplanner.tourism.iranplanner.standard.ClickableViewPager
                    android:id="@+id/EventsViewPager"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <me.relex.circleindicator.CircleIndicator
                    android:id="@+id/indicator"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true" />

            </RelativeLayout>

        </android.support.v7.widget.CardView>

        <!--<TextView-->
        <!--android:id="@+id/txtEventsTitle"-->
        <!--style="@style/StandardTitle"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentEnd="true"-->
        <!--android:layout_alignParentStart="true"-->
        <!--android:layout_alignParentTop="true"-->
        <!--android:layout_marginBottom="@dimen/margin_short"-->
        <!--android:layout_marginStart="16dp"-->
        <!--android:paddingTop="@dimen/margin_standard"-->
        <!--android:text="رویدادها" />-->
        <TextView
            android:id="@+id/txtEventsTitle"
            style="@style/StandardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/margin_short"
            android:gravity="center"
            android:paddingTop="@dimen/padding_standard"
            android:text="رویدادها" />
    </RelativeLayout>

    <!--hotelsTypeHolder-->
    <RelativeLayout
        android:id="@+id/hotelsTypeHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/viewPagerEventsHolder"
        android:padding="@dimen/padding_standard">

        <TextView
            android:id="@+id/txtHotelTypeTitles"
            style="@style/StandardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/margin_short"
            android:text="دسته بندی اقامت گاه ها" />

        <LinearLayout
            android:id="@+id/linearEventsHolderTop"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_below="@+id/txtHotelTypeTitles"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/hotelَAppartementHolderGrouping"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_xxshort"
                android:layout_weight="1">

                <TextView
                    style="@style/StandardTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="هتل آپارتمان"
                    android:textColor="@color/white_"
                    android:translationZ="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/aparteman"
                    android:scaleType="fitXY" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/hotelBoomgardiHolderGrouping"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_xxshort"
                android:layout_weight="1">

                <TextView
                    style="@style/StandardTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="بوم گردی"
                    android:textColor="@color/white_"
                    android:translationZ="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/boomgardi"
                    android:scaleType="fitXY" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearEventsHolderBottom"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_below="@+id/linearEventsHolderTop"
            android:layout_marginTop="@dimen/margin_xshort"
            android:orientation="horizontal"
            android:weightSum="2">

            <RelativeLayout
                android:id="@+id/hotelHolderGrouping"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_xxshort"
                android:layout_weight="1">

                <TextView
                    style="@style/StandardTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="هتل"
                    android:textColor="@color/white_"
                    android:translationZ="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/hotel"
                    android:scaleType="fitXY" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/hotelTraditionalHolderGrouping"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_xxshort"
                android:layout_weight="1">

                <TextView
                    style="@style/StandardTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="مهمانسرا"
                    android:textColor="@color/white_"
                    android:translationZ="1dp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/sonati"
                    android:scaleType="fitXY" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

    <!--HotelBestHolder-->
    <RelativeLayout
        android:id="@+id/viewPagerBestHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/hotelsTypeHolder"
        android:padding="@dimen/padding_standard">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerBestHotel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtBestTitleHotel"
            android:focusable="false" />

        <!--<TextView-->
        <!--android:id="@+id/txtBestTitleHotel"-->
        <!--style="@style/StandardTitle"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentStart="true"-->
