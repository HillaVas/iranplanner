        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerItinerary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtItineraryTitle"
            android:focusable="false" />

        <TextView
            android:id="@+id/txtItineraryTitle"
            style="@style/StandardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/margin_short"
            android:text="برنامه سفرها" />

    </RelativeLayout>
    <!--ItineraryHolder-->
    <RelativeLayout
        android:id="@+id/localFoodHomeHolder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/itineraryHomeHolder"
        android:padding="@dimen/padding_standard"
