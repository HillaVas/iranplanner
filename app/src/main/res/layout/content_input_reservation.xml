                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginStart="@dimen/margin_standard"
                            android:padding="@dimen/margin_short">

                            <TextView
                                android:id="@+id/txtHotelAddressValue"
                                style="@style/StandardTextGrey"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/margin_short"
                                android:layout_toEndOf="@+id/txtHotelAddress"
                                android:text="gggg" />

                            <TextView
                                android:id="@+id/txtHotelAddress"
                                style="@style/StandardTextDarkblue"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:text="آدرس" />


                        </RelativeLayout>
                    </RelativeLayout>

                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </android.support.v7.widget.CardView>

